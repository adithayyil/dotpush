{
  "manifest_version": 3,
  "name": "DotPush",
  "version": "1.0",
  "description": "Auto-push completed LeetCode problems to GitHub.",
  "permissions": [
    "scripting",
    "activeTab"
  ],
  "host_permissions": [
    "https://leetcode.com/problems/*"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
